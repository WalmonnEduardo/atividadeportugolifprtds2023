	funcao voce_venceu()
	{
		escreva("VVVVVVVV           VVVVVVVV                                            ^^          \n")
		escreva("V::::::V           V::::::V                                          ^^ ^^         \n")
		escreva("V::::::V           V::::::V                                         ^^   ^^             \n")
		escreva("V::::::V           V::::::V                                                      \n")
		escreva(" V:::::V           V:::::V ooooooooooo       cccccccccccccccc    eeeeeeeeeeee    \n")
		escreva("  V:::::V         V:::::Voo:::::::::::oo   cc:::::::::::::::c  ee::::::::::::ee  \n")
		escreva("   V:::::V       V:::::Vo:::::::::::::::o c:::::::::::::::::c e::::::eeeee:::::ee\n")
		escreva("    V:::::V     V:::::V o:::::ooooo:::::oc:::::::cccccc:::::ce::::::e     e:::::e\n")
		escreva("     V:::::V   V:::::V  o::::o     o::::oc::::::c     ccccccce:::::::eeeee::::::e\n")
		escreva("      V:::::V V:::::V   o::::o     o::::oc:::::c             e:::::::::::::::::e\n")
		escreva("       V:::::V:::::V    o::::o     o::::oc:::::c             e::::::eeeeeeeeeee\n")
		escreva("        V:::::::::V     o::::o     o::::oc::::::c     ccccccce:::::::e     \n")
		escreva("         V:::::::V      o:::::ooooo:::::oc:::::::cccccc:::::ce::::::::e          \n")
		escreva("          V:::::V       o:::::::::::::::o c:::::::::::::::::c e::::::::eeeeeeee  \n")
		escreva("           V:::V         oo:::::::::::oo   cc:::::::::::::::c  ee:::::::::::::e  \n")
		escreva("            VVV            ooooooooooo       cccccccccccccccc    eeeeeeeeeeeeee  \n")
		proximo(5000)
		escreva("VVVVVVVV           VVVVVVVV                                                                                             \n")
		escreva("V::::::V           V::::::V                                                                                             \n")
		escreva("V::::::V           V::::::V                                                                                             \n")
		escreva("V::::::V           V::::::V                                                                                             \n")
		escreva(" V:::::V           V:::::V eeeeeeeeeeee    nnnn  nnnnnnnn        cccccccccccccccc    eeeeeeeeeeee    uuuuuu    uuuuuu  \n")
		escreva("  V:::::V         V:::::Vee::::::::::::ee  n:::nn::::::::nn    cc:::::::::::::::c  ee::::::::::::ee  u::::u    u::::u  \n")
		escreva("   V:::::V       V:::::Ve::::::eeeee:::::een::::::::::::::nn  c:::::::::::::::::c e::::::eeeee:::::eeu::::u    u::::u  \n")
		escreva("    V:::::V     V:::::Ve::::::e     e:::::enn:::::::::::::::nc:::::::cccccc:::::ce::::::e     e:::::eu::::u    u::::u  \n")
		escreva("     V:::::V   V:::::V e:::::::eeeee::::::e  n:::::nnnn:::::nc::::::c     ccccccce:::::::eeeee::::::eu::::u    u::::u  \n")
		escreva("      V:::::V V:::::V  e:::::::::::::::::e   n::::n    n::::nc:::::c             e:::::::::::::::::e u::::u    u::::u  \n")
		escreva("       V:::::V:::::V   e::::::eeeeeeeeeee    n::::n    n::::nc:::::c             e::::::eeeeeeeeeee  u::::u    u::::u  \n")
		escreva("        V:::::::::V    e:::::::e             n::::n    n::::nc::::::c     ccccccce:::::::e           u:::::uuuu:::::u  \n")
		escreva("         V:::::::V     e::::::::e            n::::n    n::::nc:::::::cccccc:::::ce::::::::e          u:::::::::::::::uu\n")
		escreva("          V:::::V       e::::::::eeeeeeee    n::::n    n::::n c:::::::::::::::::c e::::::::eeeeeeee   u:::::::::::::::u\n")
		escreva("           V:::V         ee:::::::::::::e    n::::n    n::::n  cc:::::::::::::::c  ee:::::::::::::e    uu::::::::uu:::u\n")
		escreva("            VVV            eeeeeeeeeeeeee    nnnnnn    nnnnnn    cccccccccccccccc    eeeeeeeeeeeeee      uuuuuuuu  uuuu\n")
	}
}
