	funcao escreva_devagar(cadeia texto)
	{						
		caracter letra
		inteiro velocidade = 100
		inteiro j = 0
		
		para(inteiro i = 0 ; i < tx.numero_caracteres(texto) ; i++)
		{
			letra = tx.obter_caracter(texto, i)
			se(i > 0)
			{
				caracter letra_menos = tx.obter_caracter(texto, i-1)
				se(j == 149 ou letra == '[')
				{
					escreva("\n")
					j = 0
				}
			}
			escreva(letra)
			j++
			
			escolha(letra)
			{

				caso ' ': u.aguarde(0) pare				
				caso ',': u.aguarde(velocidade * 3) pare	
				caso ':': u.aguarde(velocidade * 3) pare	
				caso ';': u.aguarde(velocidade * 3) pare		
				caso '.': u.aguarde(velocidade * 5) pare			
				caso '!': u.aguarde(velocidade * 5) pare				
				caso '?': u.aguarde(velocidade * 5) pare				
				caso contrario: u.aguarde(velocidade) pare
				
			}
		}
	}
	funcao verificacao_vida()
	{
		se(vida <= 0)
		{  	
			proximo(6000)
			escreva_devagar("você morreu\n")
			fim_de_jogo()
			escreva_devagar("aos comprimentos de Walmonn e Evillyn, muito obrigada por jogar:)")
		}
		se(sanidade <= 10)
		{
			proximo(6000)
			escreva_devagar("Você ficou louco\naos comprimentos de Walmonn e Evillyn, muito obrigada por jogar:)")
			fim_de_jogo()
		}
		se(status <= 10) 
		{
			proximo(6000)
			escreva_devagar("Você se tornou odiado\naos comprimentos de Walmonn e Evillyn, muito obrigada por jogar:)")
			fim_de_jogo()
		}
		se(consequencia_ruim >= 120)
		{   
			proximo(6000)
			escreva_devagar("Você foi expulso \naos comprimentos de Walmonn e Evillyn, muito obrigada por jogar:)")
			fim_de_jogo()
		}
		se(vida == 20) 
		{
			se(pocao_feita >= 1)
			{
				escreva_devagar("*você usou a poção de cura\n*")
				vida += 80
			}   
		}
      }
     funcao verifica_vida_boss() 
     {
		se(bigboss <= 10)
		{ 
			escreva_devagar("[NARRADOR]O monstro começa a virar a gosma verde outra vez, e agora ele está indo em dirção de Tony")
          }
     }
     funcao escolha_1()
     {
     	escreva("*********************\n")
		escreva("* 1 ---> exmay      *\n")
		escreva("* 2 ---> exterminion*\n")
		escreva("*********************\n")
		leia(opcao[1])
		limpa()
		escolha(opcao[1])
		{
			caso 1:
			escreva_devagar("[NITA]EXMAY![VOCÊ]EXMAY!![VOZ]COMO OUSA ME DESOBEDECER?? [NARRADOR]Uma dor toma sua cabeça, você fica palído e luta pra ficar em pé [NITA]Tá tudo bem? [VOCÊ]Não tomei café hoje, devo estar pouco tonto(a) [NITA]Certo...aqui, já consegui sangue o suficiente, vamos ir embora, você não parece bem\n")
			sanidade = -20
			verificacao_vida()
			escreva_devagar("[NITA]Aqui, sua parte [VOCÊ]o..obriga..da obrigada [NITA]Por nada "+nome+" vê se passa lá na enfermaria, sua cara está horrível hahahahahah\n") 
			pare
			caso 2:
			escreva_devagar("[VOCÊ]EXTERMINION!! [NITA]QUAL È O SE PROBLMA HOJE?, não precisava ter machucado eles [VOCÊ]d..desculpa, não sei o que deu em mim [NITA]Espera lá fora, não quero que machuque mais nenhum deles! [VOCÊ]Foi mal nita[NARRADOR]Nita sai da caverna irritada, no caminho pra casa vocês não trocam nenhuma palavra, quando chegam no castelo ela te da um frasco com o sangue e vai embora sem dizer uma palavra\n")
			pare
		}
	}
