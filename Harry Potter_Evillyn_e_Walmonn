	funcao capitulo1()
	{
		escreva_devagar("[PROFESSOR] Bem vindo(a) a Hogwarts, pequen@ bruxo(a)[PROFESSOR] Sou o professor Alastor Moody.[ALASTOR MOODY] Qual é seu nome?\n")
		leia(nome) 
		escreva_devagar("E seu sobrenome?\n")  
		leia(sobrenome)
		escreva_devagar("[ALASTOR MOODY] Vamos então, "+ sobrenome+ ", não vai querer perder a Cerimônia de celeção[PASSOS][PASSOS]\n*Você caminha surpreso(a) com a velocidade do professor, que mesmo de bengala, o faz correr*\n*Você passou tanto tempo pensando nisso que quando percebeu ja estava nao refeitório*[PROFESSOR MACABRO] A CERIMÔNIA VAI COMEÇAR!!!!!!\n*Você vê uma garota indo em direção a um chapéu, e isso te causa estranhamento*\n*Um velho de barba esplica que esse é o chapéu celetor, ele analiza seus mais profundos desejos, e te manda pra casa que achar melhor*\n")
		proximo(6000)
		escreva_devagar("[CHAPÉU SELETOR] Grfinória!!![GAROTO] Já é a vigésima vez que cai grifinória, parece que eles deram sorte com os calouros esse ano, *Um garoto fala ao seu lado*[TONY] Aliás, prazer, me chamo Tony, *Ele diz*[TONY] Qual é, você não fala?[VOCÊ] Ah, desculpe, estava esperando ser chamad@ pra seleção de casas mas ainda não fui\n")
		escreva_devagar("[VOCÊ]Me chamo "+nome+ ".") 
		escreva_devagar("[TONY]Prazer "+nome+ ".")
		proximo(6000)
		escreva_devagar("[CARINHA QUE TA LENDO OS NOMES] Agh, como se pronúncia esse nome,"+ nome+ " venha para seleção!!![NARRADOR] Você pode tirar uma das 4 casas, são elas , Grifinória, Corvinal, Lufa-Lufa ou Sonserina, boa sorte!\n") 
		casa_sorteada()
		escreva_devagar("[TONY] Bom, parece que estamos juntos [VOCÊ] É, parece que sim")	
	} //fim conversa1
	funcao casa_sorteada()     
	{
		inteiro casa_s = u.sorteia(1, 4)
		u.aguarde(1000)
		se(casa_s == 1)
		{		
			escreva_devagar("GRIFINÓRIAA!!!!!!!!!!!!!\n") 
			casa = "GRIFINÓRIAA"	
		}
		senao se(casa_s == 2) 
		{ 		
			escreva_devagar( "CORVINAL!!!!!\n")
			casa = "CORVINAL"
		}
		senao se(casa_s == 3) 
		{ 	
			escreva_devagar("LUFA-LUFA!!!!!!!!!!\n") 
			casa = "LUFA-LUFA"
		}
		senao se(casa_s == 4) 
		{
			escreva_devagar("SONSERINA!!!!!!!!!!!!!\n") 
			casa = "SONSERINA"
		}
	}// fim casa sorteada
	funcao proximo(inteiro t)
	{
		u.aguarde(t)
		limpa()
	}
